# This is Git's per-user configuration file.
# [user]
#     name = <Daniel Topa>
#         email = dantopa@lanl.gov
#         [push]
#             default = simple
#             [http]
#                proxy = http://proxyout.lanl.gov:8080
[user]
	name = Daniel Topa
	email = dantopa@lanl.gov
	signingkey = 9A96D8FF
[push]
	default = simple
[color]
	ui = true
[rerere]
	enabled = true
[http]
[https]
[http]
[https]
[http]
	proxy = http://proxyout.lanl.gov:8080
[https]
	proxy = http://proxyout.lanl.gov:8080
[gpg]
	program = gpg2
[commit]
	gpgsign = false
	template = /Users/l127914/.stCommitMsg
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[core]
	excludesfile = /Users/l127914/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
